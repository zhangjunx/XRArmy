/* 发布对象弹出层页面结束 */
.schoolIcon{
	width: 102px;
	height: 102px;
}
.schoolLogo{
	position: absolute;
	left: 0;
	top: 0;
	width: 102px;
	height: 102px;
	opacity: 0;
}
.permissionSet{
	border: 1px solid #ddd;
}
.module{
	border-right: 1px solid #ddd;
	height: 100%;
}
.module h2{
	padding: 10px 10px;
	border-bottom: 1px solid #ddd;
	font-size: 18px;
	color: #333;
	background: #F5F7FA;
}
.module-list{
	width: 100%;
	height: calc(100% - 60px);
	overflow: auto;
	box-sizing: border-box;
	padding-bottom: 20px;
}
.module-list li{
	font-size: 18px;
	border: 1px solid #ddd;
	text-align: center;
	color: #333;
	line-height: 60px;
	width: 222PX;
	cursor: pointer;
	height: 60px;
	margin: 0 auto;
	margin-top: 20px;
	box-sizing: border-box;
	border-radius: 3px;
}
.module-list li.curr{
	color: #039BDD;
	border-color: #039BDD;
	background:url(../../img/right-role.png) no-repeat right 27px;
}

.power{
	height: 100%;
}
.power h2{
	padding: 10px 10px;
	border-bottom: 1px solid #ddd;
	font-size: 18px;
	color: #333;
	background: #F5F7FA;
}
table td .checkbox {
    height: 18px;
    line-height: 18px;
    float: left;
    display: inline-block;
    padding-left: 16px;
    width: auto;
    margin-right: 15px;
    overflow: hidden;
    background: url(../../img/form_check.png) no-repeat left 0px;
    cursor: pointer;
}
table td .checkbox.curr{
	float: left;
	display: inline-block;
	padding-left: 16px;
	width: auto;
	margin-right: 15px;
	overflow: hidden;
	background: url(../../img/form_check_on.png) no-repeat left 3px;
}
.tableBox{
	padding: 0 15px;
	box-sizing: border-box;
	height: calc(100% - 60px);
	overflow: auto;
}

/* 权限组 */
.permission-group{
	width: 100%;
	height: 100%;
	overflow: auto;
}
.permission-group .permission-item{
	padding: 20px 10px 10px 10px;
	box-sizing: border-box;
	display: inline-block;
	border-top: 3px solid #00CAD7;
	background-color: #EFF6F9;
	width: 272px;
	margin:10px;
}
.permission-group .permission-item h3{
	font-size: 16px;
	font-weight: bold;
	color: #333;
	padding-bottom: 15px;
	box-sizing: border-box;
	text-align: center;
}
.permission-group .permission-item p{
	height: 60px;
	margin-bottom: 20px;
	display:-webkit-box;
	-webkit-line-clamp:3;
	-webkit-box-orient:vertical;
	overflow:hidden;
}
.permission-group .permission-item div {
	display: flex;
	justify-content: space-between;
}
.permission-group .permission-item div span{
	cursor: pointer;
}
.permission-group .permission-item div span:hover{
	opacity: 0.6;
	transition: all ease 0.5s;
}
.permission-group .permission-item div span img{
	margin-right:5px;
}

