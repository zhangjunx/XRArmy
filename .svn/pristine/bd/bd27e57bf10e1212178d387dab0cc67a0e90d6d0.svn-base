/* 设置用户开始 */
.toolbar_all {
    font-size: 14px;
    padding: 2px 10px;
    border: 1px solid #ddd;
    border-radius: 10px;
    float: right;
}
.toolbar_empty {
    font-size: 14px;
    padding: 2px 10px;
    border: 1px solid #ddd;
    border-radius: 10px;
    float: right;
    margin-right: 20px;
}
.holderRole_dept {
    width: 300px;
    height: calc(100% - 60px);
    padding: 20px 0;
    box-sizing: border-box;
    float: left;
}
.holderRole_dept .dept_title {
    font-size: 18px;
    color: #008EDE;
    padding-left: 60px;
    background: url(../../img/wx_approver3.png) no-repeat 20px center;
    border-right: 1px solid #ddd;
}
.holderRole_dept .deptTree {
    height: calc(100% - 13px);
    overflow: auto;
    margin-left: 11px;
    border-right: 1px solid #ddd;
}
.approval_person {
    height: calc(100% - 45px);
    overflow: auto;
    padding: 10px;
    box-sizing: border-box;
}
.approval_person li {
    display: inline-block;
    border: 1px solid #ccc;
    border-radius: 5px;
    width: 260px;
    height: 110px;
    padding-left: 15px;
    box-sizing: border-box;
    overflow: hidden;
    margin: 10px;
    line-height: 110px;
    cursor: pointer;
}
.approval_person li .personPhoto {
    width: 75px;
    height: 87px;
    float: left;
}
.approval_person li .personPhoto img {
    width: 75px;
    height: 87px;
    vertical-align: middle;
}
.approval_person li .holderInfo {
    float: left;
    width: 160px;
    padding-top: 18px;
    padding-left: 20px;
    box-sizing: border-box;
}
.approval_person li .holderInfo p {
    line-height: 28px;
}
.approval_person li.cur {
    background: url(../../img/check.png) no-repeat right bottom;
    border-color: rgb(9,179,255);
}
/* 设置用户结束 */
