<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8">
  <title>新增车辆</title>
  <meta name="renderer" content="webkit">
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
  <meta name="viewport" content="width=device-width, initial-scale=1.0, minimum-scale=1.0, maximum-scale=1.0, user-scalable=0">
  <link rel="stylesheet" href="../layui/css/layui.css" media="all">
  <link rel="stylesheet" href="../css/public.css">
  <link rel="stylesheet" href="css/carManage.css">
</head>
<body>
	<input type="hidden" id="fid">
	<div class="layui-fluid">
		<div class="layui-con">
			<div class="layui-form layuiadmin-card-header-auto">
					<div class="layui-form-item">
						<div class="layui-col-lg4">
							<label class="layui-form-label">停车场编号</label>
							<div class="layui-input-block">
								<input type="text" name="code" id="code"
									autocomplete="off" lay-verify="required" class="layui-input" placeholder="请输入">
							</div>
						</div>
					</div>
					<div class="layui-form-item">
						<div class="layui-col-lg4">
							<label class="layui-form-label">停车场名称</label>
							<div class="layui-input-block">
								<input type="text" name="parkName" id="parkName"
									autocomplete="off" lay-verify="required" class="layui-input" placeholder="请输入">
							</div>
						</div>
					</div>
					<div class="layui-form-item">
						<div class="layui-col-lg4">
							<label class="layui-form-label">停车场位置</label>
							<div class="layui-input-block">
								<input type="text" name="location" id="location"
									autocomplete="off" lay-verify="required" class="layui-input" placeholder="请输入">
							</div>
						</div>
					</div>
					<div class="layui-form-item">
						<div class="layui-col-lg4">
							<label class="layui-form-label">车场类型</label>
							<div class="layui-input-block">
								<input type="text" name="type" id="type"
									autocomplete="off" lay-verify="required" class="layui-input" placeholder="请输入">
							</div>
						</div>
					</div>
					<div class="layui-form-item">
						<div class="layui-col-lg4">
							<label class="layui-form-label">容纳车辆数</label>
							<div class="layui-input-block">
								<input type="number" name="maxNum" id="maxNum"
									autocomplete="off"  class="layui-input" placeholder="请输入">
							</div>
						</div>
					</div>
					<div class="layui-form-item layui-layout-admin">
						<div class="layui-input-block">
							<button class="layui-btn layui-bg-blue" lay-submit=""
								lay-filter="component-form-demo1">确定</button>
							<button type="reset" class="layui-btn layui-btn-primary" id="resetData">取消</button>
						</div>
					</div>
			</div>
		</div>
	</div>
	<script src="../js/path.js"></script>  
	<script src="../js/jquery.min.js"></script>  
	 <script src="../js/jquery-cookie.js"></script> 
    <script src="../layui/layui.js"></script>
    <script src="js/parkingManage_add.js"></script>
</body>
</html>